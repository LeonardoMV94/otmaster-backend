{
  "name": "otmaster",
  "version": "1.0.0",
  "description": "proyecto de taller Sistema de tickets de soporte ",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "lint": "eslint"
  },
  "keywords": [
    "express",
    "mysql",
    "sequelize"
  ],
  "author": "Leonardo Munoz <contacto@pctester.cl>",
  "license": "MIT",
  "dependencies": {
    "@babel/register": "^7.18.6",
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  },
  "engines": {
    "node": "16.x"
  }
}
